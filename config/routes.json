{
    "routes": [{
            "method": "GET",
            "path": "/script",
            "handler": "Script.index",
            "config": {
                "policies": []
            }
        },
        {
            "method": "GET",
            "path": "/script/:id",
            "handler": "Script.findOne",
            "config": {
                "policies": []
            }
        },
        {
            "method": "POST",
            "path": "/script",
            "handler": "Script.store",
            "config": {
                "policies": ["plugins::strapi-plugin-python-script-executor.isSuperAdmin"]
            }
        },
        {
            "method": "PUT",
            "path": "/script/:id",
            "handler": "Script.update",
            "config": {
                "policies": ["plugins::strapi-plugin-python-script-executor.isSuperAdmin"]
            }
        },
        {
            "method": "DELETE",
            "path": "/script/:id",
            "handler": "Script.delete",
            "config": {
                "policies": ["plugins::strapi-plugin-python-script-executor.isSuperAdmin"]
            }
        }, {
            "method": "POST",
            "path": "/script/run",
            "handler": "Script.executeCommand",
            "config": {
                "policies": ["plugins::strapi-plugin-python-script-executor.isSuperAdmin"]
            }
        }
    ]
}